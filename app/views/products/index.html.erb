


<h1>Listing products</h1>

<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered"  id="products">
  <thead>
  <tr>
    <th>Title</th>
    <th>Number</th>
    <th>Price</th>
    <th>Version</th>

  </tr>
  </thead>
  <tbody>
<% @products.each do |product| %>
  <tr>
    <td><%= product.title %></td>
    <td><%= product.number %></td>
    <td><%= product.price %></td>
    <td><%= product.version %></td>

<% end %>
</tbody>
</table>

<br />

<%= link_to 'New Product', new_product_path %>
