
<h1>评审结果</h1>
<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered"  id="products">
  <thead>
  <tr>
    <th>状态</th>
    <th>标题</th>
    <th>评审人</th>
    <th>被评审人</th>
    <th>评审时间</th>
    <th>评审结果</th>
  </tr>
  </thead>
  <tbody>
  <% @pictures.each do |picture| %>
  <tr>
    <td><%= picture.status %></td>
    <td><%= picture.title %></td>
    <td><%= picture.inspecter %></td>
    <td><%= picture.user %></td>
    <td><%= picture.inspectime %></td>
    <td><%= picture.result %></td>
    <% end %>
  </tbody>
</table>

